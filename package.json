{
    "name": "mednis-expo",
    "version": "1.0.0",
    "scripts": {
        "start": "expo start",
        "tunnel": "expo start --dev-client --tunnel",
        "android": "expo run:android --device",
        "ios": "expo run:ios --device",
        "prebuild": "expo prebuild",
        "prebuild:clean": "expo prebuild --clean",
        "format": "prettier --write \"**/*.+(js|ts|tsx|json|md)\"",
        "format:check": "prettier --check \"**/*.+(js|ts|tsx|json|md)\"",
        "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
        "test": "jest",
        "test:debug": "jest -o --watch --coverage=false",
        "test:coverage": "jest --coverage",
        "typecheck": "tsc --noEmit",
        "postinstall": "patch-package",
        "licenses": "node scripts/licenses.mjs",
        "dependencies": "npm run licenses",
        "verify": "npm run typecheck && npm run test && npm run lint && npm run format:check"
    },
    "dependencies": {
        "@expo-google-fonts/ibm-plex-sans": "^0.2.2",
